<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox prefHeight="37.0" prefWidth="282.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button mnemonicParsing="false" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fx:id="match_details_goal_or" fitHeight="150.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/goal.png" />
               </image>
            </ImageView>
         </graphic>
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Button>
      <Text fx:id="firstName" strokeType="OUTSIDE" strokeWidth="0.0" text="Juraj">
         <font>
            <Font size="11.0" />
         </font>
         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
      </Text>
      <Text fx:id="surname" strokeType="OUTSIDE" strokeWidth="0.0" text="Pacholsky">
         <font>
            <Font size="11.0" />
         </font>
         <HBox.margin>
            <Insets left="4.0" top="10.0" />
         </HBox.margin>
      </Text>
      <Pane prefHeight="37.0" prefWidth="30.0" HBox.hgrow="ALWAYS" />
      <Text fx:id="time" strokeType="OUTSIDE" strokeWidth="0.0" text="78'">
         <font>
            <Font size="11.0" />
         </font>
         <HBox.margin>
            <Insets left="40.0" right="15.0" top="10.0" />
         </HBox.margin>
      </Text>
   </children>
</HBox>
