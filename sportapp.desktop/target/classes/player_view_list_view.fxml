<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox prefHeight="47.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <Button mnemonicParsing="false" style="-fx-background-color: transparent;" textOverrun="CENTER_ELLIPSIS">
                     <HBox.margin>
                        <Insets left="1.0" top="8.0" />
                     </HBox.margin>
                  </Button>
                  <VBox prefHeight="42.0" prefWidth="72.0">
                     <children>
                        <Text fx:id="nameHome11" fill="#686868" strokeType="OUTSIDE" strokeWidth="0.0" text="MSK  Zilina">
                           <font>
                              <Font size="10.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="8.0" />
                           </VBox.margin>
                        </Text>
                        <Text fx:id="nameAway11" strokeType="OUTSIDE" strokeWidth="0.0" text="Spartak Trnava">
                           <font>
                              <Font size="10.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="3.0" />
                           </VBox.margin>
                        </Text>
                     </children>
                     <HBox.margin>
                        <Insets left="3.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox prefHeight="53.0" prefWidth="18.0">
                     <children>
                        <Text fx:id="goalsHome11" strokeType="OUTSIDE" strokeWidth="0.0" text="0">
                           <font>
                              <Font size="10.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="15.0" top="9.0" />
                           </VBox.margin>
                        </Text>
                        <Text fx:id="goalsAway11" strokeType="OUTSIDE" strokeWidth="0.0" text="3">
                           <font>
                              <Font size="10.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="15.0" top="3.0" />
                           </VBox.margin>
                        </Text>
                     </children>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </VBox>
                  <Button mnemonicParsing="false" style="-fx-background-color: transparent;" text="1">
                     <font>
                        <Font size="9.0" />
                     </font>
                     <graphic>
                        <ImageView fx:id="numberOfGoals" fitHeight="150.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@img/football-ball-png-image-800.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <padding>
                        <Insets left="25.0" right="10.0" top="16.0" />
                     </padding>
                  </Button>
                  <Button mnemonicParsing="false" style="-fx-background-color: transparent;" text="1">
                     <font>
                        <Font size="9.0" />
                     </font>
                     <graphic>
                        <ImageView fx:id="numberOfYellowCards" fitHeight="150.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@img/pict--yellow-card-football-(soccer)-vector-stencils-library.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <padding>
                        <Insets left="25.0" right="10.0" top="16.0" />
                     </padding>
                  </Button>
                  <Button fx:id="numberOfRedCards" mnemonicParsing="false" style="-fx-background-color: transparent;" text="1">
                     <font>
                        <Font size="9.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="150.0" fitWidth="5.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@img/1200px-Red_card.svg.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <padding>
                        <Insets left="25.0" right="10.0" top="16.0" />
                     </padding>
                  </Button>
               </children>
            </HBox>
            <Pane prefHeight="1.0" style="-fx-background-color: lightgrey;" />
         </children>
      </VBox>
   </children>
</HBox>
